    readonly attribute DOMString domain;

    /**
     * The complete URI of the document.
     */
    readonly attribute DOMString URL;

    /**
     * The root <a>'svg'</a> in the document hierarchy.
     */
    readonly attribute SVGSVGElement rootElement;
  };

