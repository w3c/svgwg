    /**
     * Corresponds to attribute <a>'y'</a> on the given <a>'svg'</a> element.
     */
    readonly attribute SVGAnimatedLength y;

    /**
     * Corresponds to attribute <a>'width'</a> on the given <a>'svg'</a>
     * element.
